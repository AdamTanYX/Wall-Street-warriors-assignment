<AnchorPane prefHeight="488.0" prefWidth="832.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="WallStreetGUI.loginController">
   <children>
      <AnchorPane fx:id="right_pane" layoutY="-7.0" prefHeight="547.0" prefWidth="490.0">
         <children>
            <ImageView fx:id="big_image" fitHeight="217.0" fitWidth="233.0" layoutX="137.0" layoutY="216.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@GUI%20image/bull.png" />
               </image>
               <effect>
                  <Reflection bottomOpacity="0.11" fraction="0.44" topOpacity="0.39" />
               </effect>
            </ImageView>
            <Label fx:id="big_label" layoutX="84.0" layoutY="96.0" prefHeight="80.0" prefWidth="339.0" text="Bulls of WallStreet">
               <font>
                  <Font size="40.0" />
               </font>
               <effect>
                  <Glow level="0.57" />
               </effect>
            </Label>
         </children>
      </AnchorPane>
      <AnchorPane fx:id="left_pane" layoutX="489.0" layoutY="-1.0" prefHeight="538.0" prefWidth="339.0">
         <children>
            <Label fx:id="login_label" layoutX="125.0" layoutY="93.0" prefHeight="73.0" prefWidth="121.0" text="Login">
               <font>
                  <Font size="49.0" />
               </font>
               <effect>
                  <Glow />
               </effect>
            </Label>
            <Pane fx:id="email_pane" layoutX="86.0" layoutY="218.0" prefHeight="51.0" prefWidth="200.0">
               <children>
                  <TextField fx:id="email_field" layoutX="12.0" layoutY="25.0" prefHeight="25.0" prefWidth="181.0">
                     <effect>
                        <DropShadow />
                     </effect></TextField>
                  <Label fx:id="email_label" layoutX="86.0" text="Email">
                     <effect>
                        <Glow />
                     </effect></Label>
               </children>
            </Pane>
            <Pane fx:id="password_pane" layoutX="86.0" layoutY="307.0" prefHeight="51.0" prefWidth="200.0">
               <children>
                  <Label fx:id="password_label" layoutX="75.0" text="Password">
                     <effect>
                        <Glow />
                     </effect></Label>
                  <PasswordField fx:id="password_field" layoutX="11.0" layoutY="25.0" prefHeight="25.0" prefWidth="181.0">
                     <effect>
                        <DropShadow />
                     </effect></PasswordField>
               </children>
            </Pane>
            <Button fx:id="login_button" layoutX="144.0" layoutY="396.0" mnemonicParsing="false" onAction="#Login" prefHeight="31.0" prefWidth="83.0" text="Login">
               <effect>
                  <DropShadow />
               </effect></Button>
            <Button fx:id="register_button" layoutX="156.0" layoutY="481.0" mnemonicParsing="false" onAction="#Register" text="Register" textFill="#2705a4">
               <effect>
                  <Glow level="0.34" />
               </effect></Button>
            <Label fx:id="login_status" layoutX="73.0" layoutY="452.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
